class Person {
    String name;
    int age;
    public void introduce(String myname,int myage){
        this.name=myname;
        this.age=myage;
        System.out.println(name);
        System.out.println(age);
        System.out.println("wellcome  "+name +age);
    } 
    public static void main(String [] args)
    {
        Person p1=new Person();
        p1.introduce("abc",19);
    }
    }
