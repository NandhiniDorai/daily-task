class Book {
    String title;
    String author;
    public static Books(String title,String author);
    {
        this.title=title;
        this.author=author;
        System.out.print(title);
        System.out.print(author);
        
    }
    public static void main(String[] args) {
        Book b1=new Books();
        
        Book b2=Books("the reformation","o.Henry");
        
        
        
    }
}
