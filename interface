interface Playable
{
    void play();
}

class Guitar implements Playable
{
    @Override
    public void play()
    {
        System.out.println("playing guitar");
    }
}
class Piano implements Playable
{
    @Override
    public void play()
    {
        System.out.println("playing  Piano");
    }
}
public class Sample 
{
    public static void main (String[] args)
    {
        Guitar g=new Guitar();
        Piano p=new Piano();
        g.play();
        p.play();
    }
}
